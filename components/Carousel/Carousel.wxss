/* component/Carousel/Carousel.wxss */

.Carousel_wrap {
  position: relative;
  width: 100%;
  /* border-radius: 20rpx; */
  overflow: hidden;
}

.slide-image {
  display: block;
  width: 100%;
  /* border-radius: 20rpx; */
  overflow: hidden;
}

/* 首页轮播 */

.boxCell {
  position: absolute;
  width: 100%;
  height: 12rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.dot {
  width: 12rpx;
  height: 12rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 50%;
  margin-right: 10rpx;
}

.dot_active {
  background: #4c9c29;
}
